{
  "name": "qlik-embed",
  "version": "1.0.0",
  "description": "A plain web application example using the Qlik embed (webcomponents)",
  "engines": {
    "parcel": "2.x"
  },
  "keywords": [
    "qlik",
    "nebula",
    "embed",
    "webcomponent",
    "Insight",
    "Advisor",
    "mashup"
  ],
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "security": "npm audit",
    "dev": "parcel src/index.html",
    "server": "parcel serve ./src/index.html --public-url / --dist-dir dist",
    "prebuild": "npx rimraf build",
    "live": "npx live-server dist",
    "build": "rm -fr dist; parcel build ./src/index.html --dist-dir ./dist --public-url .",
    "buildanalyzer": "rm -fr dist; parcel build ./src/index.html --dist-dir ./dist --public-url . --reporter @parcel/reporter-bundle-analyzer"
  },
  "author": "QlikTech International AB - Vincent Legeard",
  "license": "ISC",
  "devDependencies": {
    "@parcel/compressor-brotli": "^2.12.0",
    "@parcel/compressor-gzip": "^2.12.0",
    "@parcel/config-default": "^2.12.0",
    "@parcel/reporter-bundle-analyzer": "^2.12.0",
    "@parcel/transformer-js": "^2.12.0",
    "@parcel/transformer-sass": "^2.12.0",
    "@popperjs/core": "^2.11.8",
    "assert": "^2.1.0",
    "bootstrap": "^5.3.3",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "events": "^3.3.0",
    "feather-icons": "^4.29.2",
    "https-browserify": "^1.0.0",
    "live-server": "^1.2.2",
    "os-browserify": "^0.3.0",
    "parcel": "2.12.0",
    "path-browserify": "^1.0.1",
    "posthtml-expressions": "^1.11.4",
    "posthtml-modules": "^0.9.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "string_decoder": "^1.3.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vm-browserify": "^1.1.2"
  },
  "dependencies": {
    "@nebula.js/sn-action-button": "1.44.5",
    "@nebula.js/sn-bar-chart": "1.35.14",
    "@nebula.js/sn-boxplot": "^0.38.5",
    "@nebula.js/sn-bullet-chart": "1.25.12",
    "@nebula.js/sn-combo-chart": "1.31.5",
    "@nebula.js/sn-distributionplot": "^0.40.8",
    "@nebula.js/sn-filter-pane": "^0.19.2",
    "@nebula.js/sn-funnel-chart": "^1.9.10",
    "@nebula.js/sn-grid-chart": "^1.16.9",
    "@nebula.js/sn-histogram": "^0.35.5",
    "@nebula.js/sn-kpi": "1.3.0",
    "@nebula.js/sn-layout-container": "3.11.1",
    "@nebula.js/sn-line-chart": "1.31.13",
    "@nebula.js/sn-map": "0.9.2",
    "@nebula.js/sn-mekko-chart": "1.4.2",
    "@nebula.js/sn-network-chart": "^1.0.4",
    "@nebula.js/sn-org-chart": "^1.1.1",
    "@nebula.js/sn-pie-chart": "1.27.10",
    "@nebula.js/sn-pivot-table": "^3.14.2",
    "@nebula.js/sn-sankey-chart": "1.10.14",
    "@nebula.js/sn-scatter-plot": "^3.57.0",
    "@nebula.js/sn-shape": "^1.1.5",
