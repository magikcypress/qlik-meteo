    "@nebula.js/sn-word-cloud": "^1.0.6",
    "@nebula.js/stardust": "5.7.1",
    "@qlik/api": "1.23.0",
    "@qlik/sdk": "^0.28.0",
    "dotenv": "^16.4.5",
    "enigma.js": "^2.14.0"
  }
}
